<section class="new">
    <%= form_for [@recipe, @recipe_food] do |form| %>
      <%= form.label :quantity %>
      <%= form.number_field :quantity %>
      <%= form.label :food_id %>
      <%= form.select :food_id, current_user.foods.map { |food| [food.name, food.id] } %>
      <%= form.submit 'Create' %>
    <% end %>
</section>